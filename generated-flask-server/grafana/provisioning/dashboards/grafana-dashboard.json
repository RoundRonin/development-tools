            "id": 1,
            "title": "Total Request Count",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "sum by (method, endpoint, http_status) (rate(request_count_total[1m]))",
                    "legendFormat": "{{method}} {{endpoint}} {{http_status}}",
                    "refId": "A"
                }
            ]
        },
        {
            "datasource": "Prometheus",
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "id": 2,
            "title": "Request Latency",
            "type": "timeseries",
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(request_latency_seconds_bucket[5m])) by (le, method, endpoint))",
                    "legendFormat": "{{method}} {{endpoint}}",
                    "refId": "A"
                }
            ]
        },
        {
            "datasource": "Prometheus",
            "gridPos": {
